{
  "extends": "@vue/tsconfig/tsconfig.json",
  "include": ["client/env.d.ts", "client/src/**/*", "client/src/**/*.vue"],
  "exclude": ["client/src/**/__tests__/*"],
  "compilerOptions": {
    "composite": true,
    "noEmit": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./client/src/*"]
    },
    "esModuleInterop": true,
  }
}